{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "todo-nginx",
      "image": "rmetel/todo-nginx:1.2",
      "hostname": "todo-nginx",
      "links": ["todo-frontend", "todo-backend"],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        },
        {
          "hostPort": 443,
          "containerPort": 443
        }
      ],
      "essential": true,
      "memory": 1024
    },
    {
      "name": "todo-frontend",
      "image": "rmetel/todo-frontend:1.6",
      "hostname": "todo-frontend",
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ],
      "memory": 1024
    },
    {
      "name": "todo-backend",
      "image": "rmetel/todo-backend:1.13",
      "hostname": "todo-backend",
      "portMappings": [
        {
          "hostPort": 5000,
          "containerPort": 8080
        }
      ],
      "memory": 2048
    }
  ]
}